program main;
    for n in [1 .. 4 ] loop
        M := {1 .. n};
        print("powerSet(", M, ") = ", powerSet(M));
    end loop;

    procedure powerSet(S);
        if S = {} then
            return { {} };
        end if;            
        x from S;
        P := powerSet(S);
        return P + { M + { x } : M in P };
    end powerSet;
end main;
