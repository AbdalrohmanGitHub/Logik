program main;

    print( ggt(42, 9) );

    -- teiler(n) berechnet die Menge aller Teiler von n.
    procedure teiler(n);
        return { i in {1 .. n} | n mod i = 0 };
    end teiler;

    -- gt(m,n) berechnet die Menge der gemeinsamen Teiler von m und n.
    procedure gt(m, n);
        return teiler(m) * teiler(n);
    end gt;

    -- ggt(m,n) berechnet den größten gemeinsamen Teiler von m und n.
    procedure ggt(m, n);
        return max/ gt(m, n);
    end ggt;

end main;
