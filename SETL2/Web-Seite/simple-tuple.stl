program main;
    a := [ 1, 2, 3 ];
    b := [ 2, 3, 4, 5, 6 ];
    c := { 5, 6, 7 };
    -- Aneinander hängen der Tupel mit +
    print(a, " + ", b, " = ", a + b);
    -- Berechnung der Anzahle der Elemente einer Menge
    print("# ", c, " = ", # c);
    -- Berechnung der Länge eines Tupels
    print("# ", a, " = ", # a);
    -- Entfernen des ersten Elements des Tupels a
    x fromb a;
    print("x = ", x);
    print("a = ", a);
    -- Entfernen des letzten Elements des Tupels b
    x frome b;
    print("x = ", x);
    print("b = ", b);
    -- Auswahl des dritten Elements von b
    print(b, "(3) = ", b(3) );
    -- Überschreiben des dritten Elements von b
    b(3) := 42;
    print("b = ", b);
end main;
