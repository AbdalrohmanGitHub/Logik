program main;
    a := { 1, 2, 3 };
    b := { 2, 3, 4 };
    -- Berechnung der Vereinigung-Menge mit +
    c := a + b;
    print(a, " + ", b, " = ", c);
    -- Berechnung der Schnitt-Menge mit     *
    c := a * b;
    print(a, " * ", b, " = ", c);
    -- Berechnung der Differenz-Menge mit   -
    c := a - b;
    print(a, " - ", b, " = ", c);
    -- Berechnung der Potenz-Menge mit      pow
    c := pow a;
    print("pow ", a, " = ", c);
end main;
