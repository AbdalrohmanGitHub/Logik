program main;
    read(n);
    m := n mod 10;
    case
        when m = 0 => print("letzte Ziffer ist 0");
        when m = 1 => print("letzte Ziffer ist 1");
        when m = 2 => print("letzte Ziffer ist 2");
        when m = 3 => print("letzte Ziffer ist 3");
        when m = 4 => print("letzte Ziffer ist 4");
        when m = 5 => print("letzte Ziffer ist 5");
        when m = 6 => print("letzte Ziffer ist 6");
        when m = 7 => print("letzte Ziffer ist 7");
        when m = 8 => print("letzte Ziffer ist 8");
        when m = 9 => print("letzte Ziffer ist 9");
        otherwise  => print("unmöglicher Fehler aufgetreten");
    end case;
end main;
