program main;

    for n in { 1 .. 9999 } loop 
        if perfect(n) then
            print(n);
        end if;
    end loop;

    procedure perfect(n);
        return 0 +/ properDivisor(n) = n;
    end perfect;

    procedure properDivisor(n);
        return { t in { 1 .. n-1 } | n mod t = 0 };
    end properDivisor;
end main;
    